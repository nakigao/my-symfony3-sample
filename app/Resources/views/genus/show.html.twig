{% extends 'base.html.twig' %}
{% block title %}
    GENUS {{ name }}
{% endblock %}
{% block body %}
    <h1>The Genus {{ name }}</h1>
    <a href="{{ path('genus_show_notes', {'genusName': name}) }}">Json Notes</a>
    <ul>
        {% for note in notes %}
            <li>{{ note }}</li>
        {% endfor %}
    </ul>
{% endblock %}